{% extends 'base.html.twig' %}

{% block title %}computer
{% endblock %}

{% block body %}
	<h1>computer</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ computer.id }}</td>
			</tr>
			<tr>
				<th>Model</th>
				<td>{{ computer.model }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ computer.description }}</td>
			</tr>

			<tr>
				<th>Prix</th>
				<td>{{ computer.prix }}</td>
			</tr>
			<tr>
				<th>Year</th>
				<td>{{ computer.year }}</td>
			</tr>
			<tr>
				<th>Imgfile</th>
				<td>{{ computer.imgfile }}</td>
			</tr>
			<tr>
				<th>reference</th>
				<td>{{ computer.ref}}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_computer_index') }}">back to list</a>

	<a href="{{ path('app_computer_edit', {'id': computer.id}) }}">edit</a>

	{{ include('computer/_delete_form.html.twig') }}
{% endblock %}
